  <ItemGroup>
    <ProjectReference Include="..\ResumeMetadataLibrary\ResumeMetadataLibrary.csproj" />
    <ProjectReference Include="..\SendGridMailSender\SendGridMailSender.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Fonts\arial.ttf">
